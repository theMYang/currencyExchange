<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Configuration 3.0//EN" "mybatis-3-mapper.dtd" >

<mapper namespace="com.wbo.currencyExchange.dao.UserLoginDao">
	
	<select id="getById" resultType="UserLogin">
		select login_name, password, user_state, mobile_phone from user_login where user_login_id= #{id}
	</select>

</mapper>